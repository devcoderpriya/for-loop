#include <stdio.h>
int main() {
    int b=1;
    int rows;
    printf("Enter the number of rows: ");
    scanf("%d", &rows);
    int column;
    printf("Enter the number of rows: ");
    scanf("%d", &column);
    
    
    int i;
    int j;
    
    for (i = 1; i <= rows; i++)
    {
        for (j = 1; j <= i; j++)
        {
           printf("%d",b);
           b++;
        }
        printf("\n");
    }

    return 0;
}

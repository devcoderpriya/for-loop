#include<stdio.h>

int main()
{
    int row;
    printf("enter the row");
    scanf("%d",&row);
    
    
    int column;
    printf("enter the row");
    scanf("%d",& column);
    int i;
    int j;
    for(i=1;i<=row;i++){
    
          for(j=1;j<=column;j++){
    
    if((i+j)%2==0){
        printf ("* ");
         }
         else {
         printf("# ");
         }
         }
         printf ("\n");
    }
    return 0;
    }

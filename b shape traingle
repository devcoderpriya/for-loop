#include <stdio.h>

int main() {
    int i, j;
    

    
    for (i = 1; i <= 3; i++) {
        for(int j=3-i;j>=1; j--){
            printf("  ");
        }
        
        for (j = 1; j <= i; j++) {
            printf("* ");
        }
        printf("\n");
    }
    
    for (i = 2; i >= 1; i--) {
        for(int j=3-i;j>=1; j--){
            printf("  ");
        }
        
        for (j = 1; j <= i; j++) {
            printf("* ");
        }
        printf("\n");
    }

    return 0;
}

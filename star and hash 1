#include <stdio.h>
int main() {
    int rows;
    printf("Enter the number of rows: ");
    scanf("%d", &rows);
    
    int i;
    int j;
    for (j = 1; j <= i; j++){
    for (i = 1; i <= rows; i++) {
         
           if (j % 2 == 1) {
                printf("* ");
            } else {
                printf("# ");
            }
        }
        printf("\n");
    }

    return 0;
}

#include<stdio.h>
int main()
{
    int base;
    printf("enter the number :");
    scanf("%d",&base);
    int power;
    printf("enter the number :");
    scanf("%d",&power);
    int result=1;
    int i;
    for(i=0;i<power;i++)
    {
        result=result*base;

    }
    printf("%d",result);
    return 0;
}

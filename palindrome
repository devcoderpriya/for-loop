#include<stdio.h>
int main()
{
    int a;
    printf("enter the number:");
    scanf("%d",&a);
    int rev=0;
    int rem;
    int originalnum;
    originalnum=a;
    for(;a!=0;a=a/10)
    {
        rem=a%10;
        rev=rev*10+rem;

    }
    if(rev==originalnum)
    {
        printf("palindrome");
    }
    else{
        printf("not palindrome");
    }
    return 0;
}

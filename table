#include<stdio.h> 
int main(){ 
    int sp, ep, table; 
    printf("Enter your starting point: "); 
    scanf("%d", &sp); 
    printf("Enter your endingpoint: "); 
    scanf("%d", &ep); 
    for(; sp<=ep; sp++){ 
        for(int j=1; j<=10; j++){ 
            table=sp*j; 
            printf("%d*%d = %d \n",sp,j,table); 
        } 
      printf("\n"); 
    } 
    return 0; 
}

nclude<stdio.h>

int main()
{
    int row;
    printf("enter the row");
    scanf("%d",&row);
    
    int column;
    printf("enter the column");
    scanf("%d",& column);
    
    int i;
    int j;
    
    for(i=1;i<=row;i++){
          for(j=1;j<=column;j++){
    
        print("*");
         }
         print("\n");
    }
    return 0;
    }
